{
  "name": "quiet-interview",
  "version": "1.0.0",
  "description": "Test app written for Quiet interview",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "docker:up": "docker compose up -d",
    "migrate:local": "prisma migrate dev",
    "migrate:local:new": "prisma migrate dev --name",
    "prisma:generate": "prisma generate",
    "start": "yarn docker:up && node ./bin/index.js",
    "start:dev": "yarn docker:up && ts-node ./src/index.ts",
    "watch": "yarn docker:up && tsc && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./bin/index.js\"",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "argon2": "^0.31.2",
    "axios": "^1.6.7",
    "cryptico": "^1.0.2",
    "dotenv": "^16.4.4",
    "ioredis": "^5.3.2",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "linked-list-typescript": "^1.0.15",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "luxon": "^3.4.4",
    "node": "^21.6.2",
    "prisma": "^5.9.1",
    "socket-io": "^1.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/axios": "^0.14.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.14.202",
    "@types/log4js": "^2.3.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
